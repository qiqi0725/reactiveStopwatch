{
    "extends": "airbnb-base",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [2, "windows"],
        "eol-last": 0,
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ]
}