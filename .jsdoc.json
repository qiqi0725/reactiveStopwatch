{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "source": {
        "include": [
            "README.md"
        ],
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "name": "Adaptive React Stopwatch",
        "referenceTitle": "Stopwatch",
        "disableSort": false,
        "collapse": true,
        "ressources": {
            "Github repo": "https://github.com/qiqi0725/reactiveStopwatch"
        },
        "footerText": "Stopwatch - Documentation"
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "./node_modules/tui-jsdoc-template"
    }
}